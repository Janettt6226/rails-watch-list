<div class="container">
  <h1 class="text-center">All lists</h1>
  <div class="row ow row-cols-4"></div>
    <% @lists.each do |list| %>
      <div class="card col" style="width: 18rem">
          <h2 class="card-title"><%= link_to list.name, list_path(list) %></h2>
      </div>
    <% end %>
  <button class="btn btn-success"><%= link_to "Add a list", new_list_path %></button>
</div>
